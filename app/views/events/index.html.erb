        </section>

        <section class="content-block team-1 team-1-1 bg-deepocean" style="color: white;">
            <div class="container">
                <div class="row">
                   <% @events.each_with_index do |event, index| %>
                  <% if event.date > Date.today + 1 %>
                    <div class="col-sm-4 team-member-wrap">
                        <div class="team-member">
                            <div class="team-details">
                              <a href="/events/<%= event.id %>">
                                <% if event.date > Date.today %>
                                  <h4 class="member-name"><%= event.date.strftime("%a, %m/%d") %></h4>
                                <% else %>
                                <h4>ENDED</h4>
                                <% end %>
                                <img src="<%= @photos["data"][index]["images"]["standard_resolution"]["url"] %>" width="100%" height="200px" /></a>
                                                                                                                                            <h3> <a href="/events/<%= event.id %>"><%= event.name %></a> </h3>
                                <p style="color: white;"><br> 9 AM - 5 PM <br> New Hampshire Motor Speedway <br> $10 to Spectate</p>
                                <ul class="social social-dark" >
                                    <div class="row">
                       
                                          <li style="display: inline-block;"><a href="<%= event.facebook_url %>"><i class="fa fa-2x fa-facebook"></i>View on Facebook</a> </li>
                                             
                                      <li style="display: inline-block;"><a href="/"><i class="fa fa-calendar-plus-o"></i>Add to Google Calendar</a></li>
                                      <br>
                                                                                             <a class="btn-sm btn btn-default center" style="width 60%; display: table; margin: 0 auto; color: red;" href="/events/<%= event.id %>">Register to Drive</a>
                                          
                               
                  
                                    </div>
                                </ul>
                                <!-- /.social -->
                            </div>
                        </div>
                    </div>
                  <% end %>
                  <% end %>
                    <!-- /.team-member-wrap -->
                    <!-- /.team-member-wrap -->
                    <!-- /.team-member-wrap -->
                </div>
                <!-- /.row -->
            </div>
            <!-- /.container -->
        </section>
        <script type="text/javascript" src="js/jquery-1.11.1.min.js"></script>         
        <script type="text/javascript" src="js/bootstrap.min.js"></script>         
        <script type="text/javascript" src="js/plugins.js"></script>
        <script src="https://maps.google.com/maps/api/js?sensor=true"></script>
        <script type="text/javascript" src="js/bskit-scripts.js"></script>         
    </body>     
</html>
