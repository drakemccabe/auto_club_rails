                    <style>
                      .calendar li {
                        display: inline-block;
                        list-style-type: none;
          
                        padding: 10px;
                        
                      }
                      
                      .buttonnav li {
                        display: inline;
                        list-style-type: none;
                        padding: 10px;
                      }
                      
                      .middle_icon {
                        height: 50px;
                        width: 50px;
                      }
                      
                      .calendar li:nth-child(-n+4) {
                        float: left;
                      }
                      
                       .calendar li:nth-child(-n+8):nth-child(n+5) {
                        float: right;
                      }
                      
                      .calbox {
                        border: 1px solid #d3d3d3;
                        margin: 10px;
                        width: 80px;
                        
                      }
                      .top {
                        top: 0px;
                        background-color: #d3d3d3;
                        text-transform: uppercase;
                      }
                      
                    .bottom {
                      
                     bottom: 0px;
                    }
